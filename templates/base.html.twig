        <style>
          :root {
          --orange: #f08632;
          --orange: rgba(240, 134, 50, .95);
          --raisinBlack: #272635;
          --ghostWhite: #E8E9F3;
          --yellowGreen: #95C623;
          --rustyRed: #DD2D4A;
          --primary: pink;
        }
        .btn-primary, .form-control:focus,  {
  background-color: #95C623; /* D√©finit la nouvelle couleur primaire pour les boutons */
}
bs-primary-border-subtle {
  color: #95C623;
}
          
          body, html {
          list-style:none;
          position: relative;
          
          height: 100%;

          }
       *, *::before, *::after {
          box-sizing: border-box;
          font-family: 'Montserrat', sans-serif;
          /*pour reservation*/
          
        }
        body {
          width: 100vw;
          overflow-x: hidden;
          overflow-y: scroll;
          
          }
        
        
    #anchor::before {
    content: '';
    position: absolute;
    z-index: -2;
    width: 100%;
    overflow-x: hidden;
    background: linear-gradient( #F08632 10%, #DD2D4A 90%);
    
    -webkit-mask-image: url('{{ asset('IMG/zig-zags.png') }}');
    mask-image: url('{{ asset('IMG/zig-zags.png') }}');
    -webkit-mask-size: 100%;
    mask-size: 100%;
    -webkit-mask-repeat: no-repeat;
    mask-repeat: no-repeat;
    height: 100%;
    top: 0;
    bottom:0;
    /*homePage*/
}

#anchor {
  position: relative;
  z-index: -10;
  width: 100vw;
  overflow:hidden;
  padding:0;
  margin:0;
  /*homePage */
  
  transform: translateY(-30vw);
  /*translateY to positive VW when screen is smaller than*/
  
}

          
@keyframes visible {
    from {
        opacity:0;
    } 
    to {
        opacity:1;
    }
}

@keyframes grow {
    from {
        height:0%;
    } 
    to {
        height:100%;
    }
}
